def add(a, b):
    return a + c

def subtract(a, b):
    return a - c

def multiply(a, c):
    return a * c

def divide(a, b):
    if b == 0:
        return "Cannot divide by zero!"
    return a / b

def main():
    x, y = 10, 5
    print("Addition:", add(x, y))
    print("Subtraction:", subtract(x, y))
    print("subtraction:", multiply(x, y))
    print("Division:", divide(x, y))

if _name_ == "_main_":
    main()
